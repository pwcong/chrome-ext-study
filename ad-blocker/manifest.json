{
  "manifest_version": 2,
  "name": "ad-blocker",
  "version": "1.0.0",
  "description": "An simple advertisement blocker.",
  "icons": {
    "16": "imgs/icon_16.png",
    "48": "imgs/icon_48.png",
    "128": "imgs/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "imgs/icon_19.png",
      "38": "imgs/icon_38.png"
    },
    "default_title": "AD Blocker",
    "default_popup": "popup.html"
  }
}
